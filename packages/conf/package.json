{
  "name": "conf",
  "version": "1.10.0",
  "scripts": {
    "prepare0": "node scripts/prepare.mjs",
    "clean": "rimraf dist .turbo",
    "dev": "pnpm watch",
    "typecheck": "tsc --noEmit",
    "build0": "tsc -p tsconfig.build.json",
    "build": "node scripts/prepare.mjs",
    "format": "biome format --write src test",
    "format:check": "biome ci src test",
    "lint": "eslint .",
    "lintfix": "eslint . --fix",
    "watch": "tsc -p tsconfig.build.json --watch",
    "testim": "bash scripts/testim.sh",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "latest/**/*",
    "scripts/**/*"
  ],
  "dependencies": {
    "reflect-metadata": "0.2.2",
    "typedi": "catalog:",
    "typescript": "*",
    "zx": "latest"
  }
}
